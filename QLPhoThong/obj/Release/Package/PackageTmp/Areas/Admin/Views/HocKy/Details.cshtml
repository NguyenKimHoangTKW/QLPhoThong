@model QLPhoThong.Models.HOCKY

@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<h2>Details</h2>

<div class="card">
    <div class="card-body">
        <h4 class="card-title">Thông tin học kỳ</h4>
        <div class="table-responsive">
            <table class="product-info-table m-t-20">
                <tbody>
                    <tr>
                        <td>Mã học kỳ:</td>
                        <td class="text-dark font-weight-semibold">@Html.DisplayFor(model => model.MaHky)</td>
                    </tr>
                    <tr>
                        <td>Tên học kỳ:</td>
                        <td>@Html.DisplayFor(model => model.TenHky)</td>
                    </tr>
                    <tr>
                        <td>Năm học:</td>
                        <td>@Html.DisplayFor(model => model.NamHoc)</td>
                    </tr>
                    <!--không đụng-->
                    <tr>
                        <td>Status:</td>
                        <td>
                            <span class="badge badge-pill badge-cyan">In Stock</span>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.MaHky }) |
    @Html.ActionLink("Back to List", "Index")
</p>
